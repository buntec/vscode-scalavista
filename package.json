{
  "name": "vscode-scalavista",
  "displayName": "Scalavista",
  "description": "IDE-like features for Scala - type/scaladoc on hover, auto-completion, goto definition, errors/warnings",
  "categories": [
    "Other"
  ],
  "publisher": "buntec",
  "author": {
    "name": "Christoph Bunte"
  },
  "license": "SEE LICENSE IN LICENSE",
  "icon": "scala-spiral.png",
  "homepage": "https://github.com/buntec/vscode-scalavista/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/buntec/vscode-scalavista.git"
  },
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.30.0"
  },
  "activationEvents": [
    "onLanguage:scala"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.scalavistaDebugDump",
        "title": "Dump scalavista debug information"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
