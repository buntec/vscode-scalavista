{
	"name": "vscode-scalavista",
	"displayName": "vscode-scalavista",
	"description": "IDE-like features for Scala",
    "publisher": "buntec",
    "license": "SEE LICENSE IN LICENSE",
	"homepage": "https://github.com/buntec/vscode-scalavista/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/buntec/vscode-scalavista.git"
	},
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:scala"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.scalavistaDebugDump",
			"title": "Dump debug information"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
